{
    "name": "bycedric/delegator",
    "type": "library",
    "description": "A nice API response helper, to build the output you want.",
    "keywords": [
        "delegator",
        "jsonapi"
    ],
    "homepage": "http://github.com/bycedric/delegator",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric van Putten",
            "email": "me@bycedric.com",
            "homepage": "http://bycedric.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "satooshi/php-coveralls": "^0.6.1",
        "psy/psysh": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "ByCedric\\Delegator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByCedric\\Delegator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
